#include <stdio.h>
int main() {
    int num, lastDigit, nextDigit;
    int isAscending = 1; // Assume ascending until proven otherwise

    printf("Enter a number: ");
    scanf("%d", &num);
    if (num < 10) {
        printf("A single-digit number is always in ascending order.\n");
        return 0;
    }

    lastDigit = num % 10;
    num = num / 10;

    while (num > 0) {
        nextDigit = num % 10;
        if (nextDigit > lastDigit) {
            isAscending = 0;
            break;
        }
        lastDigit = nextDigit;
        num = num / 10;
    }

    if (isAscending)
        printf("Digits are in ascending order.\n");
    else
        printf("Digits are not in ascending order.\n");

    return 0;
}